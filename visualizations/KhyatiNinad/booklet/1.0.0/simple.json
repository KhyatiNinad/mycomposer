{
  "render": {
    "markup": "booklet/view.html",
    "script": "booklet/view.js",
    "minimum": {
      "height": 300,
      "width": 300
    }
  },
  "dependencies": {
    "helpers": true,
    "card": false
  },
  "fields": {},
  "settings": [
    {
      "key": "showImage",
      "type": "checkbox",
      "defaultValue": true,
      "templateOptions": {
        "label": "Show Event Image",
        "required": false
      }
    },

 {
      "key": "paperStyle",
      "type": "select",
      "defaultValue": "gray",
      "templateOptions": {
        "label": "Select Paper Style",
        "options": [
          {
            "name": "Gray Handmade Paper",
            "value": "gray"
          },
          {
            "name": "Light Handmade Paper",
            "value": "light"
          },
          {
            "name": "White Handmade Paper",
            "value": "white"
          },
          {
            "name": "Slate Handmade Paper",
            "value": "slate"
          },
          {
            "name": "Coffee Handmade Paper",
            "value": "coffee"
          },
          {
            "name": "Dark Handmade Paper",
            "value": "dark"
          },
          {
            "name": "Tan Crumpled Paper",
            "value": "tan"
          },
          {
            "name": "Light Crumpled Paper",
            "value": "lightc"
          },
          {
            "name": "Writing Paper",
            "value": "writing"
          },
          {
            "name": "Legal Pad",
            "value": "legal"
          }
        ]
      }
    }

  ],
  "play" : {
      "allow": false,
      "auto": false,
      "interval": 8,
      "ignoreClick": false,
      "loop": false
  },  
  "credits": [
      {
          "text": "Khyati Ninad Majmudar",
          "url": ""
      }
  ]
}
  